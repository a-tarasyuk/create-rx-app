{
  "name": "create-rx-app",
  "version": "0.6.18",
  "description": "ReactXP Project Generator",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Alexander Tarasyuk",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/create-rx-app.git"
  },
  "bugs": {
    "url": "https://github.com/a-tarasyuk/create-rx-app/issues"
  },
  "keywords": [
    "reactxp",
    "react",
    "react-native",
    "react-native-windows",
    "reactxp project generator"
  ],
  "scripts": {
    "prepare": "npm run build",
    "format": "prettier --write \"./**/*.{ts,md}\"",
    "format:check": "prettier --list-different \"./**/*.{ts,md}\"",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --config .eslintrc --ext .ts src test",
    "test": "jest -c jest.config.js"
  },
  "dependencies": {
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/minimist": "1.2.0",
    "@types/mustache": "4.0.1",
    "@types/node": "13.7.7",
    "@types/username": "3.0.0",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "chalk": "3.0.0",
    "commander": "4.1.1",
    "enquirer": "2.3.4",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.8.1",
    "fs-extra": "8.1.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lodash": "4.17.15",
    "mustache": "4.0.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3",
    "username": "5.1.0",
    "uuid": "7.0.1"
  },
  "bin": {
    "create-rx-app": "bin/create-rx-app.js"
  },
  "engines": {
    "node": ">=8.11.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format:check && npm run build"
    }
  }
}
